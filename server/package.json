{
  "name": "photo-app-server",
  "version": "1.0.0",
  "description": "Backend server for photo upload and storage",
  "main": "server.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "tsx server.ts",
    "prestart": "npm run build && node dist/scripts/check-migrations.js && node dist/scripts/run-migrations.js",
    "dev": "nodemon --watch . --ext js,ts,json --ignore dist --exec \"npm run build && node dist/server.js\"",
    "worker": "node dist/worker.js",
    "dev:worker": "nodemon --exec ts-node worker.ts",
    "pretest": "node scripts/validate-keys.js",
    "test": "jest",
    "test:db": "node scripts/test-db-connection.js",
    "verify:env": "node scripts/verify-env.js",
    "verify:migrations": "node scripts/check-migrations.js",
    "db:migrate": "knex migrate:latest --knexfile knexfile.js",
    "db:migrate:status": "knex migrate:status --knexfile knexfile.js",
    "db:migrate:prod": "knex migrate:latest --knexfile knexfile.js --env production",
    "db:migrate:status:prod": "knex migrate:status --knexfile knexfile.js --env production",
    "test:ci": "jest",
    "test:watch": "jest --watch",
    "health": "node health-check.js",
    "lint": "eslint . --config ../eslint.config.js",
    "lint:fix": "eslint . --fix --config ../eslint.config.js",
    "test:e2e": "npm --prefix .. run test:e2e",
    "test:a11y": "npm --prefix .. run test:a11y",
    "test:perf": "k6 run ../perf/smoke.js",
    "test:docker:smoke": "node ../scripts/docker-smoke.cjs",
    "prepare": ""
  },
  "dependencies": {
    "@langchain/core": "^1.1.8",
    "@langchain/langgraph": "^1.0.1",
    "@langchain/openai": "^1.1.2",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.68.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.210.0",
    "@opentelemetry/sdk-node": "^0.210.0",
    "@supabase/supabase-js": "^2.76.1",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.61.0",
    "busboy": "^1.6.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "exiftool-vendored": "^31.1.0",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.2.1",
    "fuse.js": "^7.1.0",
    "heic-convert": "^2.1.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.1",
    "openai": "^6.8.1",
    "pg": "^8.16.3",
    "photo-app": "file:..",
    "prom-client": "^15.1.3",
    "rate-limit-redis": "^4.3.1",
    "serpapi": "^2.1.0",
    "sharp": "^0.34.4",
    "tmp": "^0.2.3",
    "ws": "^8.19.0",
    "zod": "^3.25.76"
  },
  "engines": {
    "node": ">=20.11 <23",
    "npm": ">=10"
  },
  "overrides": {
    "cookie": "0.7.0",
    "diff": "8.0.3",
    "lodash": "4.17.23",
    "jest-openapi": {
      "openapi-validator": "0.14.1"
    }
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@eslint/js": "^9.38.0",
    "@playwright/test": "^1.56.1",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.3",
    "babel-jest": "^30.2.0",
    "eslint": "^9.38.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "jest": "^29.7.0",
    "jest-openapi": "0.14.1",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/working/"
    ],
    "moduleNameMapper": {
      "^p-retry$": "<rootDir>/tests/__mocks__/p-retry.js",
      "^is-network-error$": "<rootDir>/tests/__mocks__/is-network-error.js"
    },
    "transform": {
      "^.+\\.[jt]sx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(@langchain|@langchain/langgraph|p-queue|p-timeout)/)"
    ],
    "collectCoverageFrom": [
      "**/*.js",
      "!node_modules/**",
      "!coverage/**",
      "!tests/**",
      "!working/**"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 60,
        "statements": 60,
        "functions": 55,
        "branches": 45
      }
    }
  }
}
