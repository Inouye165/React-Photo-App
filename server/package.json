{
  "name": "photo-app-server",
  "version": "1.0.0",
  "description": "Backend server for photo upload and storage",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "worker": "node worker.js",
    "dev:worker": "nodemon worker.js",
    "test": "jest",
    "test:ci": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --config ../.eslintrc.cjs",
    "lint:fix": "eslint . --fix --config ../.eslintrc.cjs"
  },
  "dependencies": {
    "@langchain/core": "^0.3.0",
    "@langchain/openai": "^0.3.0",
    "openai": "^6.3.0",
    "@supabase/supabase-js": "^2.76.1",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.61.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "exiftool-vendored": "^31.1.0",
    "express": "^4.18.2",
    "express-rate-limit": "^8.1.0",
    "express-validator": "^7.2.1",
    "heic-convert": "^2.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.1",
    "pg": "^8.16.3",
    "sharp": "^0.34.4",
    "sqlite3": "^5.1.7"
  },
  "overrides": {
    "openai": "^6.3.0"
  },
  "engines": {
    "node": ">=20.11 <21",
    "npm": ">=10"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/working/"
    ],
    "collectCoverageFrom": [
      "**/*.js",
      "!node_modules/**",
      "!coverage/**",
      "!tests/**",
      "!working/**"
    ]
  }
}
