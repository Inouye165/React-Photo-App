{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022"],
    "module": "CommonJS",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "checkJs": false,
    "noEmit": true,
    "strict": false,
    "types": ["node"]
  },
  "include": ["routes/**/*.ts"],
  "exclude": ["node_modules", "tests", "working", "coverage", "test-results"]
}
