{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022"],
    "module": "CommonJS",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "checkJs": false,
    "rootDir": ".",
    "outDir": "./dist",
    "noEmit": false,
    "strict": false,
    "types": ["node"]
  },
  "include": ["./**/*.ts", "./**/*.js"],
  "exclude": [
    "node_modules",
    "dist",
    "build",
    "tests",
    "working",
    "coverage",
    "test-results",
    "routes/photos.js",
    "routes/debug.js",
    "./routes/debug.js",
    "**/routes/debug.js"
  ]
}
