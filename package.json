{
  "name": "photo-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "npx tsx server/server.ts",
    "worker": "npm --prefix server run worker",
    "build": "vite build",
    "pretest": "node scripts/validate-keys.cjs && node scripts/repo-hygiene-gps-check.cjs",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky install",
    "check-env": "node scripts/verify-env.cjs",
    "secret-scan": "node scripts/secret-scan.cjs",
    "secret-scan:local": "gitleaks detect --source . --no-banner --redact",
    "check-privilege": "node scripts/check-privilege.cjs",
    "integration-test": "node scripts/integration-test.cjs",
    "lint": "eslint . --cache && npm run type-check && node scripts/repo-hygiene-gps-check.cjs",
    "lint:fix": "eslint . --fix --cache",
    "lint:server": "eslint server --cache",
    "lint:web": "eslint src --cache",
    "format": "eslint . --fix --cache",
    "format:check": "eslint . --cache --max-warnings 0",
    "type-check": "tsc -p tsconfig.json --noEmit && tsc --noEmit --target ES2022 --module commonjs --esModuleInterop --skipLibCheck server/tests/auth.middleware.test.ts server/tests/auth.v2.test.ts server/tests/redis.wrapper.test.ts && tsc -p server/tsconfig.typecheck.json",
    "preview": "vite preview",
    "test:e2e": "playwright test",
    "test:a11y": "playwright test e2e/a11y.*",
    "test:perf": "k6 run perf/smoke.js",
    "test:docker:smoke": "node scripts/docker-smoke.cjs",
    "test:stress": "node scripts/stress-test.cjs",
    "backfill:gps": "node scripts/backfill-gps.js",
    "test:arch": "depcruise src server --config .dependency-cruiser.cjs",
    "test:size": "npm run build && size-limit",
    "test:migrations": "node server/scripts/test-migrations-integrity.js",
    "test:maintainability": "npm run test:arch && npm run test:migrations"
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint --fix",
    "*.{ts,tsx}": [
      "eslint --fix",
      "bash -c 'npm run type-check'"
    ]
  },
  "dependencies": {
    "@excalidraw/excalidraw": "^0.18.0",
    "@supabase/supabase-js": "^2.84.0",
    "@tanstack/react-virtual": "^3.13.12",
    "@vis.gl/react-google-maps": "^1.7.1",
    "chess.js": "^1.0.0",
    "exifr": "^7.1.3",
    "heic-to": "^1.3.0",
    "heic2any": "^0.0.4",
    "idb-keyval": "^6.2.2",
    "konva": "^10.0.2",
    "lucide-react": "^0.555.0",
    "nanoid": "^5.0.9",
    "photo-app": "file:",
    "react": "^19.1.1",
    "react-chessboard": "^2.0.0",
    "react-dom": "^19.1.1",
    "react-easy-crop": "^5.5.6",
    "react-error-boundary": "^6.0.0",
    "react-image-crop": "^10.1.8",
    "react-konva": "^19.0.10",
    "react-masonry-css": "^1.0.16",
    "react-router-dom": "^7.12.0",
    "react-virtuoso": "^4.17.0",
    "roughjs": "^4.6.6",
    "stockfish": "^17.1.0",
    "y-websocket": "^1.5.4",
    "yjs": "^13.6.27",
    "zustand": "^5.0.8"
  },
  "engines": {
    "node": ">=20.11 <23",
    "npm": ">=10"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@eslint/js": "^9.36.0",
    "@playwright/test": "^1.56.1",
    "@size-limit/preset-big-lib": "^12.0.0",
    "@tailwindcss/postcss": "^4.1.14",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.0.1",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@types/twilio": "^3.19.2",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "dependency-cruiser": "^17.3.5",
    "dotenv": "^16.6.1",
    "eslint": "^9.36.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "happy-dom": "^20.0.10",
    "husky": "^8.0.3",
    "jsdom": "^27.0.1",
    "postcss": "^8.5.6",
    "size-limit": "^12.0.0",
    "sqlite3": "^5.1.7",
    "tailwindcss": "^4.1.14",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.6",
    "vitest": "^3.2.4"
  },
  "overrides": {
    "@excalidraw/excalidraw": {
      "nanoid": "3.3.11"
    },
    "@excalidraw/mermaid-to-excalidraw": {
      "nanoid": "5.0.9",
      "mermaid": "10.9.4",
      "dompurify": "3.2.4"
    },
    "mermaid": "10.9.4",
    "dompurify": "3.2.4",
    "tar": "7.5.7"
  },
  "size-limit": [
    {
      "path": "dist/assets/index-*.js",
      "limit": "500 kB"
    }
  ]
}
